{
  "description": "Map L_CMD to Ctrl in Chrome",
  "enabled": false,
  "manipulators": [
    {
      "conditions": [
        {
          "bundle_identifiers": [
            "com.google.Chrome"
          ],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "p",
        "modifiers": {
          "mandatory": [
            "left_command"
          ]
        }
      },
      "to": [
        {
          "key_code": "p",
          "modifiers": [
            "control"
          ]
        }
      ],
      "type": "basic"
    },
    {
      "conditions": [
        {
          "bundle_identifiers": [
            "com.google.Chrome"
          ],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "n",
        "modifiers": {
          "mandatory": [
            "left_command"
          ]
        }
      },
      "to": [
        {
          "key_code": "n",
          "modifiers": [
            "control"
          ]
        }
      ],
      "type": "basic"
    }
  ]
}
